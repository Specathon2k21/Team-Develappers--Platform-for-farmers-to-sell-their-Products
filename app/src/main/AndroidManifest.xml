    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AAAChat">
        <activity android:name=".auth.Phoneno_verify"/>
        <activity android:name=".auth.otp_check"/>
        <activity android:name=".startup.Seller_Page"></activity>
        <activity android:name=".startup.Profile_Page" />
        <activity android:name=".AboutUsActivity" />
        <activity android:name=".auth.PhoneLoginActivity" />
        <activity android:name=".startup.WelcomeScreenActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".buyerMainActivity" />
    </application>

</manifest>